.item-label-view {
  width: 710rpx;
  height: 180rpx;
  background: #ffffff;
  box-shadow: 0px 2px 6px 0px rgba(205, 205, 205, 0.5);
  border-radius: 20rpx;
  margin: 20rpx auto 0px auto;
  display: flex;
  justify-content: space-between;
  overflow: hidden;
  align-items: center;
}
.item-left-view {
  font-size: 32rpx;
  color: #333333;
  margin-left: 40rpx;
}
.item-right-view {
  color: #ed4d58;
  font-size: 32rpx;
  font-weight: 600;
  margin-right: 40rpx;
}
