.label-select-row-view {
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  background-color: #ffffff;
}
.row-select-item-text-view {
  font-size: 30rpx;
  color: #333333;
  margin-left: 30rpx;
  height: 90rpx;
  line-height: 90rpx;
}
.row-select-item-img-view {
  width: 40rpx;
  height: 40rpx;
  margin-right: 30rpx;
  margin-top: 8rpx;
}
.row-select-item-label-text-view {
  display: flex;
  height: 90rpx;
  line-height: 90rpx;
}
.row-select-item-time-text-view {
  font-size: 28rpx;
  margin-right: 10rpx;
}
.label-date-row-view {
  width: 100%;
  height: 60rpx;
  background: #f5f5f5;
  display: flex;
  justify-content: space-between;
}
.row-date-item-text-view {
  height: 60rpx;
  line-height: 60rpx;
  margin-left: 30rpx;
  background: #f5f5f5;
  color: #333333;
  font-size: 28rpx;
}
.row-date-item-price-view {
  height: 60rpx;
  line-height: 60rpx;
  margin-left: 30rpx;
  background: #f5f5f5;
  color: #999999;
  font-size: 26rpx;
  margin-right: 30rpx;
}
.label-default-row-view {
  width: 100%;
  background-color: #ffffff;
  overflow: hidden;
}
.label-inner-default-row-view {
  width: 690rpx;
  height: 120rpx;
  box-sizing: border-box;
  margin: 0px auto;
}
.border {
  border-bottom: 2rpx solid #e5e5e5;
}
.label-inner-row1-view {
  display: flex;
  justify-content: space-between;
  font-size: 30rpx;
  height: 40rpx;
  line-height: 40rpx;
  margin-top: 28rpx;
}
.label-inner-row2-view {
  display: flex;
  justify-content: space-between;
  height: 50rpx;
  line-height: 50rpx;
  color: #979797;
  font-size: 28rpx;
  margin-top: 10rpx;
}
.inner-button-label-view {
  height: 50rpx;
  line-height: 50rpx;
  font-size: 24rpx;
  color: #333333;
  border-radius: 24rpx;
  border: 2rpx solid #e5e5e5;
  width: 140rpx;
  text-align: center;
  margin-right: 18rpx;
}
.inner-button-label-view:active {
  opacity: 0.7;
}
.modal-mask-dialog-input-view {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 300;
  color: #fff;
}
.fixed-view-dialog-input-view {
  position: fixed;
  width: 558rpx;
  left: 96rpx;
  top: 414rpx;
  background-color: #ffffff;
  z-index: 400;
  overflow: hidden;
  border-radius: 20rpx;
}
.dialog-inner-input-view {
  width: 480rpx;
  margin: 40rpx auto 20rpx auto;
  border-radius: 20rpx;
  border: 2rpx solid #eeeeee;
  height: 90rpx;
  display: flex;
  line-height: 90rpx;
  font-size: 28rpx;
}
.dialog-inner-input-view input {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  padding: 0px 20rpx;
  box-sizing: border-box;
}
.dialog-bottom-button-label-view {
  width: 100%;
  height: 90rpx;
  display: flex;
  border-top: 2rpx solid #eeeeee;
}
.dialog-button-left-view {
  width: 50%;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  font-size: 28rpx;
  color: #333333;
  background-color: #fff;
}
.dialog-button-left-view:active {
  opacity: 0.7;
}
.dialog-button-right-view {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  font-size: 28rpx;
  color: #ffffff;
  background: linear-gradient(90deg, #ff0000 0%, #ff2e5d 100%);
}
.dialog-button-right-view:active {
  opacity: 0.7;
}
.dialog-text-label-view {
  width: 100%;
  height: 200rpx;
  text-align: center;
}
.dialog-text-flex-view {
  width: 400rpx;
  margin: 0px auto;
  display: flex;
  justify-content: space-between;
  font-size: 32rpx;
  color: #333;
  height: 50rpx;
}
.ljw-popup {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  font-size: 32rpx;
  text-align: center;
  background: rgba(0, 0, 0, 0.7);
}
.ljw-popup-conter {
  width: 560rpx;
  margin: 50% auto;
  border-radius: 20rpx;
  overflow: hidden;
}
.ljw-popup-text {
  text-overflow: ellipsis;
  word-wrap: break-word;
  width: 460rpx;
  background: #ffffff;
  color: #333333;
  padding: 50rpx 50rpx;
}
.ljw-popup-button {
  width: 560rpx;
  height: 88rpx;
  line-height: 88rpx;
  background: #ed4d58;
  color: #ffffff;
}
.position-img-view {
  width: 100rpx;
  height: 140rpx;
  position: absolute;
  top: 250rpx;
  left: 326rpx;
  z-index: 200;
}
