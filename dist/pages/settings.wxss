.defualt-row-label-view {
  display: flex;
  justify-content: space-between;
  width: 100%;
  height: 100rpx;
  font-size: 30rpx;
  background-color: #ffffff;
}
.defualt-row-label-view:active {
  opacity: 0.7;
}
.border {
  border-bottom: 2rpx solid #efefef;
}
.row-item-text-view {
  font-size: 30rpx;
  color: #333333;
  margin-left: 20rpx;
  line-height: 100rpx;
}
.row-item-img-view {
  width: 30rpx;
  height: 30rpx;
  margin: 30rpx 20rpx 0px 0px;
}
.bottom-fixed-button-view {
  position: fixed;
  bottom: 0px;
  left: 0px;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 30rpx;
  color: #ed4d58;
  text-align: center;
  background-color: #ffffff;
  width: 100%;
}
.bottom-fixed-button-view:active {
  opacity: 0.7;
}
