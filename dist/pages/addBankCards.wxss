.default-row-label-view {
  width: 100%;
  background-color: #ffffff;
}
.inner-row-label-view {
  width: 710rpx;
  height: 110rpx;
  line-height: 110rpx;
  display: flex;
  margin: 0px auto;
}
.border {
  border-bottom: 2rpx solid #efefef;
}
.inner-row-left-view {
  width: 200rpx;
  font-size: 30rpx;
  color: #333333;
}
.inner-row-right-view {
  width: 510rpx;
  display: flex;
  justify-content: space-between;
}
.inner-row-right-view picker {
  width: 100%;
}
.inner-row-right-small-view {
  width: 310rpx;
}
.picker-label-none-view {
  font-size: 28rpx;
  color: #cccccc;
}
.picker-label-view {
  color: #333333;
  font-size: 28rpx;
}
.inner-input {
  width: 100%;
  height: 110rpx;
  line-height: 110rpx;
  font-size: 28rpx;
}
.placeholder-inner-input {
  font-size: 28rpx;
  color: #cccccc;
}
.code-text-view {
  width: 300rpx;
  font-size: 30rpx;
  color: #ed4d58;
  text-align: right;
  padding-right: 20rpx;
  box-sizing: border-box;
}
.code-text-change-view {
  width: 300rpx;
  font-size: 30rpx;
  color: #cccccc;
  text-align: right;
  padding-right: 20rpx;
  box-sizing: border-box;
}
.defualt-row-submit-button-view {
  width: 630rpx;
  height: 100rpx;
  line-height: 100rpx;
  color: #ffffff;
  font-size: 32rpx;
  background: #ed4d58;
  border-radius: 50rpx;
  text-align: center;
  margin: 50rpx auto 30rpx auto;
}
.defualt-row-submit-button-view:active {
  opacity: 0.7;
}
