page {
  background-color: #ffffff;
}
.label-select-row-view {
  width: 100%;
  height: 90rpx;
  display: flex;
  justify-content: space-between;
  background-color: #ffffff;
}
.row-select-item-text-view {
  font-size: 30rpx;
  color: #333333;
  margin-left: 30rpx;
  height: 90rpx;
  line-height: 90rpx;
}
.row-select-item-img-view {
  width: 40rpx;
  height: 40rpx;
  margin-right: 30rpx;
  margin-top: 8rpx;
}
.row-select-item-label-text-view {
  display: flex;
  height: 90rpx;
  line-height: 90rpx;
}
.row-select-item-time-text-view {
  font-size: 28rpx;
  margin-right: 10rpx;
}
.label-date-row-view {
  width: 100%;
  height: 60rpx;
  background: #f5f5f5;
  display: flex;
  justify-content: space-between;
}
.row-date-item-text-view {
  height: 60rpx;
  line-height: 60rpx;
  margin-left: 30rpx;
  background: #f5f5f5;
  color: #333333;
  font-size: 28rpx;
}
.row-date-item-price-view {
  height: 60rpx;
  line-height: 60rpx;
  margin-left: 30rpx;
  background: #f5f5f5;
  color: #999999;
  font-size: 26rpx;
  margin-right: 30rpx;
}
.label-vs-row-view {
  width: 710rpx;
  margin: 0px auto;
}
.default-row-label-vs-view {
  display: flex;
  width: 100%;
  height: 150rpx;
  border-radius: 10rpx;
  background: #f7f7f7;
  box-shadow: 0px 8rpx 16rpx -6rpx #e3e3e3;
  color: #ed4d58;
  justify-content: space-around;
  align-items: center;
  margin: 30rpx 0px;
}
